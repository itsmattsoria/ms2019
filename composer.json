{
  "require": {
    "aelvan/imager": "v2.3.1",
    "angellco/spoon": "3.5.2",
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/cms": "3.4.5",
    "craftcms/redactor": "2.5.0",
    "mmikkel/cp-field-inspect": "1.0.7",
    "monachilada/craft-matrixtoolbar": "1.0.6",
    "verbb/smith": "1.0.0",
    "verbb/super-table": "2.4.4",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
